{
  "name": "@komine/types",
  "version": "1.0.0",
  "description": "Shared type definitions for Komine Cemetery CRM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./enums": {
      "types": "./dist/enums.d.ts",
      "import": "./dist/enums.js",
      "require": "./dist/enums.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "komine",
    "cemetery",
    "crm",
    "types"
  ],
  "license": "UNLICENSED",
  "private": true
}
